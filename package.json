{
  "name": "streamdatabase-clone",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build && rsync -avz dist/ /var/www/badges-web",
    "lint": "eslint .",
    "preview": "vite preview",
    "move": "cp -r server/middleware/* /root/badges-web/server/middleware/ && cp -r server/data/* /root/badges-web/server/data/ && cp -r server/routes/* /root/badges-web/server/routes/ && cp -r server/utils/* /root/badges-web/server/utils/"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "html2canvas": "^1.4.1",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.10.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "vite": "^4.4.5"
  }
}